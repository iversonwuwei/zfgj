	<select id="getAgent" parameterType="com.dlfc.zfgj.entity.AgtEmpInfo" resultMap="myResultMap">
		select DISTINCT
		c.id,
		<!-- 如果经纪人电话为空则取用户电话 -->
		IFNULL(c.PHONE,a.mobile) mobile,
		b.name personName,
		d.name companyName,
		c.avatar_id avatar,
		c.OFFICE_ID,
		c.grade,
		e.file_path filePath,
<!-- 		有效且未锁定且审核通过且出租信息已上架 -->
		( SELECT COUNT(1) FROM
			HOU_HOUSE_INFO h
		LEFT JOIN hou_lease_info l
		ON h.id = l.HID
		WHERE h.DELETE_FLG = 0
		AND l.DELETE_FLG = 0
		AND h.ACTIVE_STATUS != 2
		AND h.LOCK_STATUS != 1
		AND h.AUDIT_STATUS = 2
		AND l.RELEASE_STATUS = 1
		AND l.AUDIT_STATUS = 1
		AND h.BEID = c.id ) HOUSE_COUNT,
		(SELECT COUNT(E.ID) FROM CON_CONTRACT_LOG E WHERE E.EID = c.id AND E.DELETE_FLG = 0 AND E.CON_STATUS_CURR = 5) CONTRACT_COUNT
		from agt_emp_info         c   
		left join usr_user        a   on a.id = c.user_id 
		left join sys_person      b   on a.per_id = b.id
		left join agt_comp_info   d   on c.company_id = d.id
		left join sys_info_att    e   on c.USER_ID = e.lid and e.file_type = #{fileType}
		where  c. STATUS = 0 and a.DELETE_FLG = '0' and (d.LOCK_FLAG = '0' or d.LOCK_FLAG is NULL) 
			and c.DELETE_FLG = '0' and b.DELETE_FLG = '0' and d.DELETE_FLG = '0' 
			and c.id = #{id}	
	</select>
	
