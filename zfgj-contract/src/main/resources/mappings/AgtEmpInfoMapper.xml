	<select id="selectAgentCount" parameterType="com.dlfc.zfgj.entity.AgtEmpInfo" resultType="int">
		select count(1) from
		(
		SELECT DISTINCT
			c.id,
			c.user_id,
			c.grade,
			c.sort,
			(
				SELECT
					COUNT(g.ID)
				FROM
					CON_CONTRACT_LOG g
				WHERE
					g.BEID = c.id
				AND g.DELETE_FLG = 0
				AND g.CON_STATUS_CURR = 5
			) CONTRACT_COUNT
		FROM
			agt_emp_info c 
		left JOIN usr_user a ON a.id = c.user_id 
		left JOIN sys_person b ON c.pid = b.id 
		left join agt_comp_info d ON c.company_id = d.id 
		left JOIN sys_office_area_link e ON c.OFFICE_ID = e.OFFICE_ID and e.DELETE_FLG = '0'
		left JOIN sys_trade_areas f ON e.AREA_ID = f.id and f.DELETE_FLG = '0'
		WHERE  c. STATUS = 0 and a.DELETE_FLG = '0' and (d.LOCK_FLAG = '0' or d.LOCK_FLAG is NULL) 
			and c.DELETE_FLG = '0' and b.DELETE_FLG = '0' and d.DELETE_FLG = '0' 
		<if test="id != null and id != ''">
			and c.id = #{id}
		</if>
		<if test="companyId != null and companyId != ''">
			and d.id = #{companyId}
		</if>
		<if test="personName != null and personName !=''">
			and b.name like '%${personName}%'
		</if>
		<if test="districtDist != null ">
			and f.trade_id = #{districtDist}
		</if>
		<if test="districtArea != null ">
			and f.parent_id = #{districtArea}
		</if>
		<if test="companySpell != null and companySpell != ''">
			and left(d.s_spelling,1) in (${companySpell})
		</if>
		) h 
	</select>
	
